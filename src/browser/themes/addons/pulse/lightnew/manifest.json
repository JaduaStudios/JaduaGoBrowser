{
  "manifest_version": 2,

  "applications": {
    "gecko": {
      "id": "pulse-light-new@browser.fushra.com"
    }
  },

  "name": "Jadua Plus Light",
  "description": "Jadua Go's new light theme",
  "author": "Jadua Studios",
  "version": "1.2",

  "icons": { "32": "icon.svg" },

  "theme": {
    "colors": {
      "tab_background_text": "rgb(51, 47, 55)", // Grey 800
      "tab_selected": "rgb(242, 241, 244)", // Grey 100
      "tab_text": "rgb(26, 23, 28)", // Grey 900

      "icons": "rgb(23, 23, 28)", // Grey 900
      "frame": "#DDE1EB", // Grey 200
      "popup": "#D4D9E6", // Grey 200
      "popup_text": "rgb(23, 23, 28)", // Grey 900
      "popup_border": "transparent",
      "popup_highlight": "#e0e0e6", // TODO: What
      "popup_highlight_text": "#15141a", // TODO: what
      "tab_line": "transparent",

      "toolbar": "rgb(241, 242, 244)", // Grey 100
      "toolbar_top_separator": "transparent",
      "toolbar_bottom_separator": "rgb(241, 242, 244)", // Grey 100
      "toolbar_field": "rgba(221, 225, 235, 0.98)", // Grey 200
      "toolbar_field_text": "rgb(23, 23, 28)", // Grey 900
      "toolbar_field_border": "transparent",
      "toolbar_field_focus": "#DDE1EB", // Grey 200
      "toolbar_text": "rgb(23, 23, 28)", // Grey 900

      "ntp_background": "rgb(242, 241, 244)", // Grey 200
      "ntp_text": "rgb(23, 23, 28)", // Grey 900

      "button": "rgb(185, 184, 193, .33)", // Grey 300, 33% opacity
      "button_hover": "rgba(185, 184, 193, .66)", // Grey 300, 66% opacity
      "button_active": "rgb(185, 184, 193)", // Grey 300
      "button_primary": "rgba(21, 112, 239, 1)", // Primary 500, 20%
      "button_primary_hover": "rgb(38, 121, 239, 1.00)", // Primary 500, 30%
      "button_primary_active": "rgb(65, 137, 239, 1.00)", // Primary 500, 40%
      "button_primary_color": "rgb(242, 241, 244)", // Primary 100

      "error_text_color": "rgb(197,0,66)", // TODO: Error buttons

      "input_color": "rgb(208, 212, 221)", // Grey 200
      "input_background": "rgb(242, 241, 244)", // Grey 100
      "input_border_error": "#E22850", // TODO: Error buttons

      "autocomplete_popup_hover": "rgb(185, 184, 193)", // Grey 300
      "autocomplete_popup_separator": "rgb(185, 184, 193)", // Grey 300
      "appmenu_update_icon_color": "#2AC3A2", // TODO
      "appmenu_info_icon_color": "#0090ED", // TODO
      "tab_icon_overlay_stroke": "rgb(255,255,255)", // TODO: What is this
      "tab_icon_overlay_fill": "rgb(91,91,102)", // TODO: What is this
      "sidebar": "transparent",
      "sidebar_border": "transparent",

      "pulse_grey_900": "rgb(13, 14, 15)",
      "pulse_grey_850": "rgb(21, 23, 26)",
      "pulse_grey_800": "rgb(40, 43, 48)",
      "pulse_grey_700": "rgb(72, 77, 87)",
      "pulse_grey_600": "rgb(93, 100, 112)",
      "pulse_grey_500": "rgb(119, 127, 143)",
      "pulse_grey_400": "rgb(148, 159, 179)",
      "pulse_grey_300": "rgb(174, 187, 209)",
      "pulse_grey_200": "rgb(205, 221, 247)",
      "pulse_grey_100": "rgb(212, 228, 255)",

      "pulse_primary_900": "rgb(1, 7, 15)",
      "pulse_primary_800": "rgb(3, 18, 38)",
      "pulse_primary_700": "rgb(6, 30, 64)",
      "pulse_primary_600": "rgb(14, 76, 161)",
      "pulse_primary_500": "rgb(17, 94, 199)",
      "pulse_primary_400": "rgb(22, 120, 255)",
      "pulse_primary_300": "rgb(97, 163, 255)",
      "pulse_primary_200": "rgb(186, 215, 255)",
      "pulse_primary_100": "rgb(240, 246, 255)",

      "pulse_secondary_900": "rgb(1, 7, 15)",
      "pulse_secondary_800": "rgb(3, 18, 38)",
      "pulse_secondary_700": "rgb(6, 30, 64)",
      "pulse_secondary_600": "rgb(14, 76, 161)",
      "pulse_secondary_500": "rgb(17, 94, 199)",
      "pulse_secondary_400": "rgb(22, 120, 255)",
      "pulse_secondary_300": "rgb(97, 163, 255)",
      "pulse_secondary_200": "rgb(186, 215, 255)",
      "pulse_secondary_100": "rgb(240, 246, 255)"
    },
    "properties": {
      "arrowpanel_border_radius": "4px",
      "arrowpanel_menuitem_border_radius": "4px",

      "panel_hover": "color-mix(in srgb, currentColor 9%, transparent)",
      "panel_active": "color-mix(in srgb, currentColor 14%, transparent)",
      "panel_active_darker": "color-mix(in srgb, currentColor 25%, transparent)",
      "toolbar_field_icon_opacity": "1",

      "tab_rounding_size": "8px",
      "tab_rounding_size_neg": "-8px",
      "tab_block_margin": "0px",
      "tab_border_radius": "0px",
      "tab_padding_inline": "0px",

      "gradient": "linear-gradient(var(--gradient-orientation), var(--pulse-primary-500) 0%, var(--pulse-secondary-500) 100%)",
      "zap_gradient": "var(--gradient)",

      "toolbar_button_border_radius": "4px"
    }
  },

  "theme_experiment": {
    "stylesheet": "experiment.css",
    "colors": {
      "button": "--button-bgcolor",
      "button_hover": "--button-hover-bgcolor",
      "button_active": "--button-active-bgcolor",
      "button_primary": "--button-primary-bgcolor",
      "button_primary_hover": "--button-primary-hover-bgcolor",
      "button_primary_active": "--button-primary-active-bgcolor",
      "button_primary_color": "--button-primary-color",
      "error_text_color": "--error-text-color",
      "input_background": "--input-bgcolor",
      "input_color": "--input-color",
      "input_border": "--input-border-color",
      "input_border_error": "--input-error-border-color",
      "autocomplete_popup_separator": "--autocomplete-popup-separator-color",
      "appmenu_update_icon_color": "--panel-banner-item-update-supported-bgcolor",
      "appmenu_info_icon_color": "--panel-banner-item-info-icon-bgcolor",
      "tab_icon_overlay_stroke": "--tab-icon-overlay-stroke",
      "tab_icon_overlay_fill": "--tab-icon-overlay-fill",

      "pulse_grey_900": "--pulse-grey-900",
      "pulse_grey_850": "--pulse-grey-850",
      "pulse_grey_800": "--pulse-grey-800",
      "pulse_grey_700": "--pulse-grey-700",
      "pulse_grey_600": "--pulse-grey-600",
      "pulse_grey_500": "--pulse-grey-500",
      "pulse_grey_400": "--pulse-grey-400",
      "pulse_grey_300": "--pulse-grey-300",
      "pulse_grey_200": "--pulse-grey-200",
      "pulse_grey_100": "--pulse-grey-100",

      "pulse_primary_900": "--pulse-primary-900",
      "pulse_primary_800": "--pulse-primary-800",
      "pulse_primary_700": "--pulse-primary-700",
      "pulse_primary_600": "--pulse-primary-600",
      "pulse_primary_500": "--pulse-primary-500",
      "pulse_primary_400": "--pulse-primary-400",
      "pulse_primary_300": "--pulse-primary-300",
      "pulse_primary_200": "--pulse-primary-200",
      "pulse_primary_100": "--pulse-primary-100",

      "pulse_secondary_900": "--pulse-secondary-900",
      "pulse_secondary_800": "--pulse-secondary-800",
      "pulse_secondary_700": "--pulse-secondary-700",
      "pulse_secondary_600": "--pulse-secondary-600",
      "pulse_secondary_500": "--pulse-secondary-500",
      "pulse_secondary_400": "--pulse-secondary-400",
      "pulse_secondary_300": "--pulse-secondary-300",
      "pulse_secondary_200": "--pulse-secondary-200",
      "pulse_secondary_100": "--pulse-secondary-100"
    },
    "properties": {
      "arrowpanel_border_radius": "--arrowpanel-border-radius",
      "arrowpanel_menuitem_border_radius": "--arrowpanel-menuitem-border-radius",

      "panel_hover": "--panel-item-hover-bgcolor",
      "panel_active": "--arrowpanel-dimmed-further",
      "panel_active_darker": "--panel-item-active-bgcolor",
      "toolbar_field_icon_opacity": "--urlbar-icon-fill-opacity",

      "tab_rounding_size": "--tab-rounding-size",
      "tab_rounding_size_neg": "--tab-rounding-size-neg",
      // The padding around each tab. In proton, this is what makes them float
      "tab_block_margin": "--tab-block-margin",
      // The border radius of the tab
      "tab_border_radius": "--tab-border-radius",
      // Padding to the left and right of the tab. **Pulse Browser custom**
      "tab_padding_inline": "--tab-padding-inline",

      "gradient": "--gradient",
      "zap_gradient": "--panel-separator-zap-gradient",

      "toolbar_button_border_radius": "--toolbarbutton-border-radius"
    }
  }
}
