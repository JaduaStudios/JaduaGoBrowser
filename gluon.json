{
  "name": "Jadua Go",
  "vendor": "Jadua Studios",
  "appId": "com.jadua.go",
  "binaryName": "jadua-go-browser",
  "version": {
    "product": "firefox",
    "version": "124.0.2"
  },
  "buildOptions": {
    "windowsUseSymbolicLinks": false
  },
  "addons": {
    "ublock": {
      "platform": "github",
      "id": "uBlock0@raymondhill.net",
      "repo": "gorhill/uBlock",
      "version": "1.57.2",
      "fileGlob": "uBlock0_*.firefox(.signed)?.xpi"
    },
    "sidebar-tabs": {
      "platform": "github",
      "id": "sidebartabs@browser.fushra.com",
      "repo": "Jadua1010/sidebar-tabs",
      "version": "v0.6.5",
      "fileGlob": "release.xpi"
    },
    "GoPanic": {
      "id": "gopanic@jaduastudios.com",
      "platform": "github",
      "repo": "JaduaStudios/GoPanic",
      "version": "v0.1.1",
      "fileGlob": "release.xpi"
    },
    "BetterTyping": {
      "id": "bettertyping@jaduastudios.com",
      "platform": "github",
      "repo": "JaduaStudios/BetterTyping",
      "version": "v0.1.2",
      "fileGlob": "release.xpi"
    }
  },
  "brands": {
    "dev": {
      "backgroundColor": "#2B2A33",
      "brandShorterName": "Jadua Go",
      "brandShortName": "Jadua Go Browser",
      "brandFullName": "Jadua Go Browser Dev",
      "release": {
        "displayVersion": "0.3.3-1",
        "github": {
          "repo": "JaduaStudios/JaduaGoBrowser"
        },
        "x86": {
          "windowsMar": "windows.mar",
          "macosMar": "macosIntel.mar",
          "linuxMar": "linux.mar"
        }
      }
    }
  },
  "license": {
    "ignoredFiles": [
      ".*\\.json"
    ],
    "licenseType": "MPL-2.0"
  },
  "updateHostname": "goupdate.jaduastudios.com"
}